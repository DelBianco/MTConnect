{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Coleta</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nome</th>
                <td>{{ entity.nome }}</td>
            </tr>
            <tr>
                <th>Dataitems</th>
                <td>{{ entity.dataItems | join }}</td>
            </tr>
            <tr>
                <th>Probe</th>
                <td>{{ entity.probe }}</td>
            </tr>
            <tr>
                <th>Numdecoletas</th>
                <td>{{ entity.numDeColetas }}</td>
            </tr>
            <tr>
                <th>Datadecriacao</th>
                <td>{{ entity.dataDeCriacao|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('coleta') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('coleta_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
